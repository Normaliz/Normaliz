#TESTS = unittests

#noinst_PROGRAMS = unittests
#unittests_SOURCES = tests-main.cpp tests-dynamic_bitset.cpp
#unittests_LDADD = $(top_builddir)/source/libnormaliz.la
#noinst_HEADERS = catch.hpp

check-local:
	-rm -rf run_tests
	mkdir run_tests
	(cd $(srcdir) && tar cf - $(NMZTESTS)) | (cd run_tests && tar xf -)
	(cd run_tests && $(MAKE) -f $(abs_srcdir)/Makefile.classic $(NMZ_TEST_TARGETS) NORMALIZ=$(abs_top_builddir)/source/normaliz$(EXEEXT))

clean-local:
	$(MAKE) -f $(srcdir)/Makefile.classic clean
	-rm -rf run_tests

distclean-local:
	$(MAKE) -f $(srcdir)/Makefile.classic distclean
	-rm -rf run_tests

